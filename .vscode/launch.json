{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Memory Search (Simple)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/simple_debug_search.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PAPR_LOG": "debug"
            },
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false,
            "stopOnEntry": false,
            "args": []
        },
        {
            "name": "Debug Memory Search (Comprehensive)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/debug_memory_search.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PAPR_LOG": "debug"
            },
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false,
            "stopOnEntry": false,
            "args": []
        },
        {
            "name": "Debug Memory Search (Interactive)",
            "type": "python",
            "request": "launch",
            "module": "pdb",
            "args": ["${workspaceFolder}/simple_debug_search.py"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PAPR_LOG": "debug"
            },
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false
        },
        {
            "name": "Debug Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PAPR_LOG": "debug"
            },
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false,
            "stopOnEntry": false
        },
        {
            "name": "Debug Memory Search (Async)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/debug_async_search.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PAPR_LOG": "debug"
            },
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false,
            "stopOnEntry": false
        },
        {
            "name": "Debug with Custom Query",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/debug_custom_search.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PAPR_LOG": "debug"
            },
            "envFile": "${workspaceFolder}/.env",
            "justMyCode": false,
            "stopOnEntry": false,
            "args": ["${input:searchQuery}"]
        },
        {
            "name": "Convert Qwen → Core ML (fp16)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/convert_qwen_coreml.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "VIRTUAL_ENV": "${workspaceFolder}/.venv",
                "PATH": "${workspaceFolder}/.venv/bin:${env:PATH}"
            },
            "justMyCode": false,
            "args": [
                "--hf", "Qwen/Qwen3-Embedding-4B",
                "--out", "${workspaceFolder}/coreml/Qwen3-Embedding-4B.mlpackage",
                "--fp16"
            ]
        },
        {
            "name": "Convert Qwen → Core ML (int8)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/convert_qwen_coreml.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "VIRTUAL_ENV": "${workspaceFolder}/.venv",
                "PATH": "${workspaceFolder}/.venv/bin:${env:PATH}"
            },
            "justMyCode": false,
            "args": [
                "--hf", "Qwen/Qwen3-Embedding-4B",
                "--out", "${workspaceFolder}/coreml/Qwen3-Embedding-4B.mlpackage",
                "--int8"
            ]
        }
    ],
    "inputs": [
        {
            "id": "searchQuery",
            "description": "Enter your search query",
            "default": "Find information about recent projects and technical challenges",
            "type": "promptString"
        }
    ]
}
